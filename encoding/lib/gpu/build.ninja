ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=enclib_gpu -DTORCH_API_INCLUDE_EXTENSION_H -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include/torch/csrc/api/include -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include/TH -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include/THC -isystem /usr/local/cuda/include -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/include/python3.6m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++11
cuda_flags = -DTORCH_EXTENSION_NAME=enclib_gpu -DTORCH_API_INCLUDE_EXTENSION_H -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include/torch/csrc/api/include -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include/TH -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/lib/python3.6/site-packages/torch/lib/include/THC -isystem /usr/local/cuda/include -isystem /mnt/lustre/liujianbo/Data_t1/anaconda3/envs/pytorch1.0/include/python3.6m -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --compiler-options '-fPIC' --expt-extended-lambda -std=c++11
ldflags = -shared -L/usr/local/cuda/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out
  depfile = $out.d
  deps = gcc

rule cuda_compile
  command = $nvcc $cuda_flags -c $in -o $out

rule link
  command = $cxx $in $ldflags -o $out

